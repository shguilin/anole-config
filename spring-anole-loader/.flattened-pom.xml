      <groupId>org.springframework</groupId>
      <artifactId>spring-context</artifactId>
      <version>[5.1.17.RELEASE, )</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>8</source>
          <target>8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
